{
    "plugins": ["ci/jsdoc/markdown"],
    "markdown": {
        "parser": "gfm",
        "hardwrap": true
    },
    "opts": {
    	"default"         : {
			"outputSourceFiles" : true
		},
    	"template": "doc-static/docstrap/template",
    	"tutorials": "doc-static/tutorials"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true,
        "theme": "readable"
    }
}